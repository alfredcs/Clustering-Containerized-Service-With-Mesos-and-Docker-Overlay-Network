{
  "id": "tensorflow",
  "cpus": 1,
  "mem": 2048,
  "constraints": [["hostname", "UNIQUE"]],
  "instances": 1,
  "protocol": "tcp",
  "labels": {
    "HAPROXY_GROUP":"external",
    "__HAPROXY_0_VHOST__(i.e. to fprce using tcp instead of http mode in haproxy.cfg)":"cirros.vms.crd.ge.com",
    "HAPROXY_MODE":"tcp"
  },
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "dcos1-bootstrap.vms.crd.ge.com:5000/tensorflow:0.90",
      "network": "BRIDGE",
       "type": "DOCKER",
       "portMappings": [
          { "containerPort": 8888, "hostPort": 0, "servicePort": 58888, "protocol": "tcp" }
       ]
    },
    "volumes": [
          {
		"containerPath": "/etc/docker.tar.gz",
		"hostPath": "/opt/nfs1/docker.tar.gz",
		"mode": "RO"
	  }
    ]
  },
  "__cmd__": "while sleep 60; do date -u +%T; done",
  "healthChecks": [
        {
	  "protocol": "TCP",
          "portIndex": 0,
          "path": "/",
          "gracePeriodSeconds": 120,
          "intervalSeconds": 60,
          "timeoutSeconds": 5,
          "maxConsecutiveFailures": 3
        }
  ],
  "uris": [
  	"file:///opt/nfs1/auth/docker.tar.gz"
  ]
}
