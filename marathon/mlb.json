{
  "id": "mlb1.2",
  "args": [ "sse", "-m", "http://master.mesos:8080", "--health-check", "--group", "external" ],
  "cpus": 2,
  "mem": 1024,
  "constraints": [["hostname", "UNIQUE"]],
  "instances": 1,
  "acceptedResourceRoles": ["slave_public"],
  "labels": {
    "HAPROXY_GROUP":"external",
    "__HAPROXY_0_VHOST__":"mlb.vms.crd.ge.com",
    "HAPROXY_MODE":"tcp"
  },
  "ports": [80,443,9090,9091],
  "requirePorts" : true,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "dcos-bootstrap.vms.crd.ge.com:5000/mlb:1.2",
      "network": "HOST",
      "volumes": [],
      "privileged": true,
      "parameters": [],
      "forcePullImage": false
    }
  },
  "env": {
      "HAPROXY_SSL_CERT": "",
      "VIP": "10.10.5.200",
      "PRIORITY": "200",
      "INTERFACE": "eth0",
      "HAPROXY_SYSCTL_PARAMS": "net.ipv4.tcp_tw_reuse=1 net.ipv4.tcp_fin_timeout=30 net.ipv4.tcp_max_syn_backlog=10240 net.ipv4.tcp_max_tw_buckets=400000 net.ipv4.tcp_max_orphans=60000 net.core.somaxconn=1000"
  },
  "__cmd__": "while sleep 60; do date -u +%T; done",
  "backoffSeconds": 1,
  "backoffFactor": 1.15,
  "maxLaunchDelaySeconds": 3600,
  "healthChecks": [
        {
	  "protocol": "HTTP",
          "portIndex": 2,
          "path": "/monitor",
          "gracePeriodSeconds": 60,
          "intervalSeconds": 15,
          "timeoutSeconds": 2,
	  "ignoreHttp1xx": false,
          "maxConsecutiveFailures": 3
        }
  ],
  "uris": [
        "file:///opt/nfs1/docker.tar.gz"
  ]
}
